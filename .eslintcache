ion.href.split('?')[1];\r\nvar club;\r\nvar trainingDay;\r\nvar loggedIn;\r\nfunction loadLogged() {\r\n\r\n    loggedIn = JSON.parse(localStorage.getItem('loggedIn'));\r\n}\r\nfunction loadClub() {\r\n\r\n    var clubs = JSON.parse(localStorage.getItem('clubs'))\r\n\r\n    console.log(clubs)\r\n\r\n    for (let i = 0; i < clubs['clubs'].length; i++) {\r\n        if (clubs['clubs'][i].name === clubString)\r\n            club = clubs['clubs'][i]\r\n    }\r\n    console.log(club)\r\n\r\n\r\n\r\n\r\n}\r\nfunction getTrainingDay() {\r\n    switch (club.trainingDay) {\r\n        case '0':\r\n            trainingDay = \"Sunday\";\r\n            break;\r\n        case '1':\r\n            trainingDay = \"Monday\";\r\n            break;\r\n        case '2':\r\n            trainingDay = \"Tuesday\";\r\n            break;\r\n        case '3':\r\n            trainingDay = \"Wednesday\";\r\n            break;\r\n        case '4':\r\n            trainingDay = \"Thursday\";\r\n            break;\r\n        case '5':\r\n            trainingDay = \"Friday\";\r\n            break;\r\n        case '6':\r\n            trainingDay = \"Saturday\";\r\n    }\r\n\r\n}\r\n\r\n\r\nexport default class MoreInfoComponent extends Component {\r\n\r\n\r\n    render() {\r\n        loadClub();\r\n        getTrainingDay();\r\n        loadLogged();\r\n        if (loggedIn === null | loggedIn === undefined) {\r\n            return (<LandComponent />)\r\n        }\r\n        else {\r\n            return (\r\n                <div className='main moreinfo'>\r\n                    <SideBarCompnent />\r\n                    <h1>{club.name}</h1>\r\n                    <div className='moreInfoClub'>\r\n\r\n                        <img src={club.image} alt='' />\r\n                        <div className='info'>\r\n                            <p> <b>Facility:</b> {club.facility}</p>\r\n                            <p> <b>Max Size:</b> {club.maxSize}</p>\r\n                            <p> <b>Trainer: </b>{club.trainer}</p>\r\n                            <p> <b>Season: </b>{club.operatingDate}</p>\r\n                            <p> <b>Training Duration:</b> {club.trainingDuration}</p>\r\n                            <p> <b>Training Day: </b>{trainingDay}</p>\r\n                            <p><b>Calories Burnt at Training: </b> {club.calories}</p>\r\n                        </div>\r\n                    </div>\r\n                </div >\r\n            )\r\n        }\r\n\r\n    }\r\n}","C:\\Users\\micha\\OneDrive\\Documents\\CIT\\4th Year\\Sem 1\\Front end frameworks\\proj1SportsArena\\sports-arena\\src\\components\\pages\\clubs.js",[],"C:\\Users\\micha\\OneDrive\\Documents\\CIT\\4th Year\\Sem 1\\Front end frameworks\\proj1SportsArena\\sports-arena\\src\\components\\pages\\booking.js",[],"C:\\Users\\micha\\OneDrive\\Documents\\CIT\\4th Year\\Sem 1\\Front end frameworks\\proj1SportsArena\\sports-arena\\src\\components\\pages\\deleteBooking.js",[],"C:\\Users\\micha\\OneDrive\\Documents\\CIT\\4th Year\\Sem 1\\Front end frameworks\\proj1SportsArena\\sports-arena\\src\\components\\pages\\book.js",["180"],"import React, { Component } from 'react';\r\nimport SideBarCompnent from '../sidebar';\r\nimport LandComponent from './land';\r\n\r\nimport './book.css';\r\n\r\nvar bookingString = window.location.href.split('?')[1];\r\nvar club;\r\nvar loggedIn;\r\nvar bookings;\r\nfunction loadBooking() {\r\n    bookings = JSON.parse(localStorage.getItem('bookings'));\r\n    console.log(bookings)\r\n\r\n\r\n}\r\nfunction loadLogged() {\r\n\r\n    loggedIn = JSON.parse(localStorage.getItem('loggedIn'));\r\n}\r\n\r\nfunction loadClub() {\r\n\r\n    var clubs = JSON.parse(localStorage.getItem('clubs'))\r\n\r\n    //  console.log(clubs)\r\n\r\n    for (let i = 0; i < clubs['clubs'].length; i++) {\r\n        if (clubs['clubs'][i].name === bookingString)\r\n            club = clubs['clubs'][i]\r\n    }\r\n    // console.log(club)\r\n\r\n\r\n\r\n\r\n}\r\n\r\n\r\nexport default class BookComponent extend